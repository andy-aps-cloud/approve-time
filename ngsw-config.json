{
  "index": "/index.html",
  "appData": {
    "updateMessage": "Update Home & Team screens"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/favicon.png",
          "/index.html",
          "/icon-192x192.png",
          "/*.css",
          "/*.js",
          "/mobile1.jpg","/mobile2.jpg","/mobile3.jpg"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-calls",
      "urls": [
        "https://api.myjson.com/bins/1bbudh",
        "https://api.myjson.com/bins/11t269"
      ],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "1d",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}