{
  "index": "/index.html",
  "appData": {
    "updateMessage": "Checking for update every minute!"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-calls",
      "urls": [
        "https://api.myjson.com/bins/1bbudh",
        "https://api.myjson.com/bins/11t269"
      ],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    },
    {
      "name": "router-calls",
      "urls": [
        "/",
        "tabs/employees",
        "/employee-details/*"
      ],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ]
}